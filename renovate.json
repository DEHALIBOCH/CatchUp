{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "prHourlyLimit": 10,
  "packageRules": [
    {
      "matchManagers": ["maven"],
      "description": "Disable snapshot versions",
      "matchCurrentVersion": "/.*-SNAPSHOT/",
      "enabled": "false"
    }
  ]
}
